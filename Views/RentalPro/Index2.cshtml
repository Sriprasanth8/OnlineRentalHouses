@model IEnumerable<Floor.Models.RentalPro>

<div class="col-12 heading-section text-center mt-6">
    <div class="mt-5">
        <span class="subheading text-info">Posts</span>
        <h2 class="mb-4">List Of All Property</h2>
    </div>
</div>
<div class="p-5 mt-3">

    @if (Model.Count() > 0)
    {

<div class="table-responsive">
    <table class="table w-100">
        <thead>
            <tr>
                <th>Property Id</th>
                <th>Property Holder Name</th>
                <th>Property Type</th>
                <th>Square.Ft</th>
                <th>BedRoom</th>
                <th>Furnished</th>
                <th>Parking</th>
                <th>Availability</th>
                <th>HouseAmenities</th>
                <th>SocietyAmenities</th>
                <th>Image</th>
                <th>Description</th>
                <th>Rent</th>
                <th>SecurityDeposit</th>
                <th>Rental WithOut</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Created Date\time</th>
                <th>Updated Date\time</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
            <tr>
                <td class="text-center">
                    <a class="btn btn-info mx-2 border-0" asp-route-id="@obj.Id" asp-controller="RentalPro" asp-action="Details" target="_blank">@obj.Id</a>
                </td>
                <td>@obj.PropertyHolderName</td>
                <td>@obj.ProCategory.Name</td>
                <td>@obj.SquareFt</td>
                <td>@obj.BedRoom</td>
                <td>@obj.ProFurnished.Name</td>
                <td>@obj.ProParking.Name</td>
                <td>@obj.ProAvailability.Name</td>
                <td>@obj.HouseAmenities</td>
                <td>@obj.SocietyAmenities</td>
                <td>@obj.Image</td>
                <td>@obj.Description</td>
                <td>@obj.Rent</td>
                <td>@obj.SecurityDeposit</td>
                <td>@obj.ProRentalWO.Name</td>
                <td>@obj.Address</td>
                <td>@obj.City</td>
                <td>@obj.State</td>
                <td>@obj.CreateDt</td>
                <td>@obj.UpdateDt</td>
                <td class="text-center">
                    <div class="btn-group" role="group">
                        <a asp-route-Id="@obj.Id" asp-action="Upsert" class="btn btn-primary mx-2 border-0">
                            <i class="fas fa-edit"></i>
                        </a>
                        <a asp-route-Id="@obj.Id" asp-action="Delete" class="btn btn-danger mx-2 border-0">
                            <i class="far fa-trash-alt"></i>
                        </a>
                    </div>
                </td>
            </tr>
}
        </tbody>
    </table>
</div>}
else
{
<p> No property exists.</p>}

</div>

